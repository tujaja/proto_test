<table class="record">
<tbody>

<% @users.each do |user| %>
  <tr>
    <td class="id"><%=h user.id %></td>
    <td class="name"><%=h user.login %></td>
    <td class="desc"><%=h user.email %></td>
    <td class="date">
      <span class="mark"><%=h to_japan_time(user.created_at) %></span>
    </td>
    <td class="gadget">
      <%= link_to (image_tag "update.png", :alt => "update"), edit_admin_user_path(user) %>
      <%#= link_to (image_tag "delete.png", :alt => "delete"), admin_user_path(user), :confirm => 'Are you sure?', :method => :delete %>
    </td>
  </tr>
<% end %>
</tbody>
</table>
