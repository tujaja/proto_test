<div id="listing_contents">
<% if contents.any? %>
  <% contents.each do |content| %>
    <%= render :partial => "thumb_content", :locals => { :content => content } %>
    <%#= render :partial => "record_content", :locals => { :content => content } %>
  <% end %>
<% end %>

<div class="clear"></div>
</div>

