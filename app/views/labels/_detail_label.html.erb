<div id="detail" class="label">

  <h3><%=h label.name %></h3>
  <h4><%=h label.subname %></h4>

  <%= thumb_for label.images[0] %>

  <dl class="description"><dt>Description</dt><dd><%=h label.description %></dd></dl>

  <dl class="url"><dt>URL</dt> <dd><%=h label.url %></dd></dl>
  <dl class="mail"><dt>email</dt> <dd><%=h label.mail %></dd></dl>
  <dl class="address"><dt>Address</dt> <dd><%=h label.address %></dd></dl>
  <dl class="phone"><dt>Phone</dt> <dd><%=h label.phone %></dd></dl>

</div>


